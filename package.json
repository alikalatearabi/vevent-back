{
  "name": "vevent-back",
  "version": "0.0.1",
  "description": "Minimal NestJS app",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "prisma:generate": "prisma generate",
    "prisma:dbpush": "prisma db push",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "seed:users": "ts-node -r tsconfig-paths/register prisma/seed-users.ts",
    "seed:categories": "ts-node -r tsconfig-paths/register prisma/update-persian-categories.ts",
    "seed:attendees": "ts-node -r tsconfig-paths/register prisma/seed-attendees.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.0.0",
    "@prisma/client": "^5.10.0",
    "@types/minio": "^7.1.0",
    "@types/uuid": "^10.0.0",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "luxon": "^3.7.2",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.13",
    "prisma": "^5.10.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  }
}
